package com.tts.plugin.adapter.impl.base.vo;

import com.tts.message.trade.TradeMessage.Transaction;
import com.tts.plugin.adapter.api.dialect.vo.QuoteVo;

public class TradeRequestWithQuoteVo {
	
	private final Transaction transactionMessage;
	private final QuoteVo     quote;
	
	
	public TradeRequestWithQuoteVo(Transaction transactionMessage, QuoteVo quote) {
		super();
		this.transactionMessage = transactionMessage;
		this.quote = quote;
	}


	public Transaction getTransactionMessage() {
		return transactionMessage;
	}


	public QuoteVo getQuote() {
		return quote;
	}
	
	

	
}
