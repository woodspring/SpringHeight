package com.tts.mde.spot.vo;

public class MdConditionVo {
	private final long lastReceivedTime;
	private final int validBidDataCount;
	private final int validAskDataCount;
	
	public MdConditionVo(long lastReceivedTime, int validBidDataCount, int validAskDataCount) {
		super();
		this.lastReceivedTime = lastReceivedTime;
		this.validBidDataCount = validBidDataCount;
		this.validAskDataCount = validAskDataCount;
	}

	public long getLastReceivedTime() {
		return lastReceivedTime;
	}

	public int getValidBidDataCount() {
		return validBidDataCount;
	}

	public int getValidAskDataCount() {
		return validAskDataCount;
	}
	
}
